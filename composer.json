{
  "name": "agorakit/agorakit",
  "description": "Agorakit, groupware for citizen.",
  "keywords": ["groupware", "laravel"],
  "license": "AGPL",
  "type": "project",


  "require": {
    "php": "^7.3",
    "barryvdh/laravel-translation-manager": "^0.5",
    "consoletvs/charts": "7.*",
    "cviebrock/eloquent-sluggable": "^8",
    "cviebrock/eloquent-taggable": "^8",
    "ddeboer/imap": "^1",
    "fideloper/proxy": "^4.4",
    "geocoder-php/nominatim-provider": "^5",
    "guzzlehttp/guzzle": "^7.0.1",
    "intervention/image": "^2",
    "intervention/imagecache": "^2",
    "laravel/framework": "^8.0",
    "laravel/helpers": "^1",
    "laravel/tinker": "^2",
    "laravel/ui": "^3",
    "laravelcollective/html": "^6",
    "laravolt/avatar": "^4",
    "league/html-to-markdown": "^5",
    "michelf/php-markdown": "^1",
    "misd/linkify": "^1.1",
    "mpratt/embera": "~2.0",
    "nicolaslopezj/searchable": "^1",
    "rap2hpoutre/laravel-log-viewer": "1.7",
    "spatie/icalendar-generator": "^2.1",
    "spatie/laravel-honeypot": "^2",
    "suin/php-rss-writer": "^1.6",
    "toin0u/geocoder-laravel": "^4",
    "venturecraft/revisionable": "1.*",
    "watson/validating": "^6",
    "willdurand/email-reply-parser": "^2.9",
    "zbateson/mail-mime-parser": "^1.3"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3",
    "facade/ignition": "^2",
    "fzaninotto/faker": "^1.9.1",
    "laravel/browser-kit-testing": "^6",
    "laravel/tinker": "^2",
    "mmo/faker-images": "^0.3",
    "mockery/mockery": "^1",
    "nunomaduro/collision": "^5",
    "phpunit/phpunit": "^9.0"
  },



  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "files": [
      "app/Helpers/Functions.php",
      "app/Helpers/Filters.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "classmap": [
      "tests/TestCase.php",
      "tests/BrowserKitTestCase.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
