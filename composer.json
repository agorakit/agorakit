{
  "name": "philippejadin/agorakit",
  "description": "Agorakit, groupware for citizen.",
  "keywords": ["groupware", "laravel"],
  "license": "GPL",
  "type": "project",


  "require": {
    "php": "^7.2.5",
    "consoletvs/charts": "6.*",
    "cviebrock/eloquent-sluggable": "^7",
    "cviebrock/eloquent-taggable": "^7",
    "ddeboer/imap": "^1.10",
    "eluceo/ical": "^0.16",
    "fideloper/proxy": "^4.2",
    "fruitcake/laravel-cors": "^1.0",
    "geocoder-php/mapbox-provider": "^1",
    "geocoder-php/nominatim-provider": "^5",
    "guzzlehttp/guzzle": "^6.3",
    "intervention/image": "2.*",
    "intervention/imagecache": "2.*",
    "laravel/framework": "^7.0",
    "laravel/helpers": "^1.2",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^2.0",
    "laravelcollective/html": "6.*",
    "misd/linkify": "^1.1",
    "mpratt/embera": "~1.0",
    "nicolaslopezj/searchable": "^1.10",
    "rap2hpoutre/laravel-log-viewer": "1.4",
    "spatie/laravel-honeypot": "^2",
    "suin/php-rss-writer": "^1.6",
    "toin0u/geocoder-laravel": "^4.1",
    "venturecraft/revisionable": "1.*",
    "watson/validating": "^5",
    "willdurand/email-reply-parser": "^2.8"
  },
  "require-dev": {
    "facade/ignition": "^2.0",
    "fzaninotto/faker": "^1.9.1",
    "mockery/mockery": "^1.3.1",
    "nunomaduro/collision": "^4.1",
    "phpunit/phpunit": "^8.5",
    "kkomelin/laravel-translatable-string-exporter": "^1.2",
    "barryvdh/laravel-debugbar": "^3",

    "beyondcode/laravel-dump-server": "^1.0",
    "beyondcode/laravel-self-diagnosis": "^1",

    "laravel/browser-kit-testing": "^6",
    "laravel/tinker": "^2"
  },



  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "files": [
      "app/Helpers/Functions.php",
      "app/Helpers/Filters.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "classmap": [
      "tests/TestCase.php",
      "tests/BrowserKitTestCase.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
