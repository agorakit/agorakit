{
    "name": "agorakit/agorakit",
    "description": "Agorakit, groupware for citizen.",
    "keywords": [
        "groupware",
        "laravel"
    ],
    "license": "AGPL-3.0-only",
    "type": "project",
    "require": {
        "php": "^8.3",
        "consoletvs/charts": "^6.7",
        "cviebrock/eloquent-sluggable": "*",
        "cviebrock/eloquent-taggable": "*",
        "ddeboer/imap": "*",
        "doctrine/dbal": "^3.0",
        "geocoder-php/nominatim-provider": "*",
        "guzzlehttp/guzzle": "*",
        "intervention/image": "*",
        "konekt/html": "^6.5",
        "laravel/framework": "^11.0",
        "laravel/helpers": "*",
        "laravel/tinker": "*",
        "laravel/ui": "^4.0",
        "laravolt/avatar": "*",
        "league/html-to-markdown": "*",
        "mews/purifier": "*",
        "michelf/php-markdown": "*",
        "misd/linkify": "*",
        "mpratt/embera": "*",
        "nesbot/carbon": "^3.8",
        "nicolaslopezj/searchable": "*",
        "rap2hpoutre/laravel-log-viewer": "*",
        "spatie/icalendar-generator": "*",
        "spatie/laravel-honeypot": "*",
        "spatie/laravel-ignition": "^2.0",
        "suin/php-rss-writer": "*",
        "tio/laravel": "*",
        "toin0u/geocoder-laravel": "*",
        "venturecraft/revisionable": "*",
        "watson/validating": "*",
        "willdurand/email-reply-parser": "*",
        "zbateson/mail-mime-parser": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "*",
        "fakerphp/faker": "*",
        "laravel/browser-kit-testing": "*",
        "mmo/faker-images": "*",
        "mockery/mockery": "*",
        "nunomaduro/collision": "^8.1",
        "larastan/larastan": "*",
        "phpunit/phpunit": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        },
        "platform": {
            "php": "8.3"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/Functions.php",
            "app/Helpers/Filters.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/TestCase.php",
            "tests/BrowserKitTestCase.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "scan": "vendor/bin/phpstan analyse app tests --level 0 --memory-limit 1G",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
