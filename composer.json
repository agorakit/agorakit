{
  "name": "philippejadin/agorakit",
  "description": "Agorakit, groupware for citizen.",
  "keywords": ["groupware", "laravel"],
  "license": "GPL",
  "type": "project",


  "require": {
    "php": ">=7.1.3",
    "barryvdh/laravel-translation-manager": "0.5.6",
    "consoletvs/charts": "5.4.0",
    "cviebrock/eloquent-sluggable": "4.8",
    "cviebrock/eloquent-taggable": "3.5.3",
    "ddeboer/imap": "1.9.0",
    "eluceo/ical": "0.15.1",
    "facade/ignition": "1.12.1",
    "fideloper/proxy": "4.2.1",
    "geocoder-php/mapbox-provider": "1.0.1",
    "geocoder-php/nominatim-provider": "5.1.1",
    "intervention/image": "2.5.1",
    "intervention/imagecache": "2.4.0",
    "kkomelin/laravel-translatable-string-exporter": "1.7.0",
    "laravel/framework": "5.8.35",
    "laravelcollective/html": "5.8.1",
    "misd/linkify": "1.1.4",
    "mpratt/embera": "1.9.3",
    "nicolaslopezj/searchable": "1.12.0",
    "rap2hpoutre/laravel-log-viewer": "0.22.1",
    "spatie/laravel-honeypot": "1.4.0",
    "suin/php-rss-writer": "1.6.0",
    "tio/laravel": "1.9",
    "toin0u/geocoder-laravel": "4.2.4",
    "venturecraft/revisionable": "1.33.0",
    "watson/validating": "3.3.0",
    "willdurand/email-reply-parser": "2.8.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "3.2.9",
    "beyondcode/laravel-dump-server": "1.4.0",
    "beyondcode/laravel-self-diagnosis": "1.3.2",
    "fzaninotto/faker": "1.9.1",
    "laravel/browser-kit-testing": "5.1.3",
    "laravel/homestead": "8.6.0",
    "laravel/tinker": "1.0.10",
    "mockery/mockery": "1.3.1",
    "nunomaduro/collision": "2.1.1",
    "phpunit/phpunit": "7.5.20"
  },



  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "files": [
      "app/Helpers/Functions.php",
      "app/Helpers/Filters.php"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "classmap": [
      "tests/TestCase.php",
      "tests/BrowserKitTestCase.php"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
