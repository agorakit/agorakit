{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "beyondcode/laravel-self-diagnosis": "^1.0",
        "consoletvs/charts": "5.*",
        "cviebrock/eloquent-sluggable": "4.5",
        "cviebrock/eloquent-taggable": "~3.3",
        "eluceo/ical": "^0.14.0",
        "fideloper/proxy": "~4.0",
        "intervention/image": "^2.4",
        "intervention/imagecache": "2.*",
        "kwi/urllinker": "dev-master",
        "laravel/framework": "5.6.*",
        "laravel/socialite": "^3.1",
        "laravelcollective/html": "5.6.*",
        "rap2hpoutre/laravel-log-viewer": "^0.19",
        "suin/php-rss-writer": "^1.6",
        "toin0u/geocoder-laravel": "^4.0",
        "venturecraft/revisionable": "1.*",
        "watson/autologin": "^0.5",
        "watson/validating": "^3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/browser-kit-testing": "^4.0",
        "laravel/tinker": "~1.0",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/Functions.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/TestCase.php",
            "tests/BrowserKitTestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
